    "async": "^2.3.0",
    "axios": "^0.16.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "jimp": "^0.2.27",
    "jsonwebtoken": "^7.3.0",
    "mime": "^1.3.4",
    "mongoose": "^4.9.4",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "uuid": "^3.0.1"
